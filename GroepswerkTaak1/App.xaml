<Application
	x:Class="GroepswerkTaak1.App"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:c="clr-namespace:GroepswerkTaak1.ToBeDeleted"
	xmlns:helpers="clr-namespace:GroepswerkTaak1.Helpers"
	xmlns:local="clr-namespace:GroepswerkTaak1"
	StartupUri="Views/winLogon.xaml">
	<!--  StartupUri="Views/MainWindow.xaml" StartupUri="Views/winLogon.xaml"  -->

	<Application.Resources>
		<!--  Kleuren  -->
		<SolidColorBrush x:Key="MainBackgroundColor" Color="#FFCDDDDD" />

		<SolidColorBrush x:Key="ThemeColor1" Color="#FF3BBCBD" />

		<SolidColorBrush x:Key="ThemeColor2" Color="#FFD75563" />

		<SolidColorBrush x:Key="ThemeColor3" Color="#FFE84848" />

		<SolidColorBrush x:Key="ThemeColor4" Color="#FFdb5265" />

		<SolidColorBrush x:Key="ThemeColor5" Color="#88D7D8" />

		<!--  Menu-item stijl  -->
		<Style x:Key="CustomMenuItemStyle" TargetType="MenuItem">
			<Setter Property="HorizontalContentAlignment" Value="Center" />
			<Setter Property="VerticalContentAlignment" Value="Center" />
			<Setter Property="FontSize" Value="14" />
		</Style>

		<!--  Afmelden knop stijl  -->
		<Style x:Key="AfmeldenButtonStyle" TargetType="Button">
			<Setter Property="Background" Value="{StaticResource ThemeColor1}" />
			<Setter Property="Margin" Value="10,2" />
			<Setter Property="Padding" Value="2" />
			<Setter Property="Width" Value="80" />
			<Setter Property="FontSize" Value="14" />
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="Button">
						<Border
							Background="{TemplateBinding Background}"
							BorderBrush="Gray"
							BorderThickness="1"
							CornerRadius="5">
							<ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
						</Border>
						<ControlTemplate.Triggers>
							<Trigger Property="IsMouseOver" Value="True">
								<Setter Property="Background" Value="{StaticResource ThemeColor2}" />
							</Trigger>
						</ControlTemplate.Triggers>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>
		<!--  StyleKnoppengrid  -->
		<Style x:Key="GridButton" TargetType="Button">
			<Setter Property="Background">
				<Setter.Value>
					<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
						<GradientStop Offset="0.0" Color="#60c8ca" />
						<GradientStop Offset="1.0" Color="#0aabad" />
					</LinearGradientBrush>
				</Setter.Value>
			</Setter>
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="Button">
						<Border
							Background="{TemplateBinding Background}"
							BorderBrush="{TemplateBinding BorderBrush}"
							BorderThickness="{TemplateBinding BorderThickness}"
							CornerRadius="5">

							<!--  This enables binding to Button.Content or ContentTemplate  -->
							<ContentPresenter
								HorizontalAlignment="Center"
								VerticalAlignment="Center"
								RecognizesAccessKey="True" />
						</Border>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>

		<!--  Style Transparant button  -->
		<Style x:Key="TransparentButton" TargetType="Button">
			<Setter Property="Background" Value="Transparent" />
			<Setter Property="BorderBrush" Value="Transparent" />
			<Setter Property="VerticalAlignment" Value="Center" />
			<Setter Property="Padding" Value="10" />
			<Setter Property="Margin" Value="5" />
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="Button">
						<Border
							x:Name="btnBorder"
							Background="{TemplateBinding Background}"
							BorderBrush="{TemplateBinding BorderBrush}"
							BorderThickness="{TemplateBinding BorderThickness}"
							CornerRadius="5"
							SnapsToDevicePixels="True">
							<ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
						</Border>
						<ControlTemplate.Triggers>
							<Trigger Property="IsMouseOver" Value="True">
								<Setter TargetName="btnBorder" Property="Background" Value="#20000000" />
							</Trigger>
							<Trigger Property="IsPressed" Value="True">
								<Setter TargetName="btnBorder" Property="Background" Value="#40000000" />
								<Setter TargetName="btnBorder" Property="BorderBrush" Value="Black" />
							</Trigger>
							<Trigger Property="IsEnabled" Value="False">
								<Setter TargetName="btnBorder" Property="Opacity" Value="0.3" />
							</Trigger>
						</ControlTemplate.Triggers>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>

		<!--  Stijl van de repeat buttons(lege ruimtes op de schuifbalk) in ScrollBar  -->
		<Style x:Key="ScrollBarRepeatButtonStyle" TargetType="RepeatButton">
			<Setter Property="HorizontalAlignment" Value="Stretch" />
			<Setter Property="VerticalAlignment" Value="Stretch" />
			<Setter Property="Background" Value="Transparent" />
			<Setter Property="BorderThickness" Value="0" />
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="RepeatButton">
						<Border Background="{TemplateBinding Background}" />
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>

		<!--  Stijl van ScrollBar  -->
		<Style x:Key="CustomVerticalScrollBar" TargetType="ScrollBar">
			<Setter Property="Orientation" Value="Vertical" />
			<Setter Property="Width" Value="14" />
			<Setter Property="Background" Value="{StaticResource ThemeColor1}" />
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="ScrollBar">
						<Grid Background="{StaticResource ThemeColor1}">
							<Grid.RowDefinitions>
								<RowDefinition Height="Auto" />
								<RowDefinition Height="*" />
								<RowDefinition Height="Auto" />
							</Grid.RowDefinitions>
							<RepeatButton
								Grid.Row="0"
								Height="14"
								Background="Transparent"
								BorderBrush="Transparent"
								Command="ScrollBar.LineUpCommand"
								CommandTarget="{Binding RelativeSource={RelativeSource TemplatedParent}}">
								<Path
									Data="M 7 4 L 4.15 10 L 9.85 10 Z"
									Fill="{StaticResource ThemeColor5}"
									Stretch="Uniform" />
							</RepeatButton>
							<Track
								x:Name="PART_Track"
								Grid.Row="1"
								IsDirectionReversed="True">
								<Track.DecreaseRepeatButton>
									<RepeatButton IsEnabled="False" Style="{StaticResource ScrollBarRepeatButtonStyle}" />
								</Track.DecreaseRepeatButton>
								<Track.Thumb>
									<Thumb
										Width="10"
										Margin="0"
										Background="{StaticResource ThemeColor5}"
										BorderThickness="0">
										<Thumb.Template>
											<ControlTemplate TargetType="Thumb">
												<Border
													Background="{TemplateBinding Background}"
													BorderThickness="{TemplateBinding BorderThickness}"
													CornerRadius="4" />
											</ControlTemplate>
										</Thumb.Template>
									</Thumb>
								</Track.Thumb>
								<Track.IncreaseRepeatButton>
									<RepeatButton IsEnabled="False" Style="{StaticResource ScrollBarRepeatButtonStyle}" />
								</Track.IncreaseRepeatButton>
							</Track>
							<RepeatButton
								Grid.Row="2"
								Height="14"
								Background="Transparent"
								BorderBrush="Transparent"
								Command="ScrollBar.LineDownCommand"
								CommandTarget="{Binding RelativeSource={RelativeSource TemplatedParent}}">
								<Path
									Data="M 4.15 4 L 9.85 4 L 7 10 Z"
									Fill="{StaticResource ThemeColor5}"
									Stretch="Uniform" />
							</RepeatButton>
						</Grid>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>

